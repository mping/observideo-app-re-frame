{
  "name": "observideo",
  "homepage": "https://observideo.com",
  "version": "0.0.19",
  "description": "Observideo is an app for categorizing video segments",
  "license": "MIT",
  "main": "resources/main.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "dev": "shadow-cljs watch main renderer",
    "build-dev": "shadow-cljs compile main renderer",
    "build": "shadow-cljs compile main renderer",
    "pom": "shadow-cljs pom",
    "clean": "rm -rf resources/public/js/* && rm -rf dist && rm -rf target",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dist-mwl": "electron-builder -mwl"
  },
  "build": {
    "appId": "com.observideo.app",
    "files": [
      "resources/**/*",
      ".shadow-cljs/**/*",
      "**/*",
      "!.idea",
      "!src",
      "!.git",
      "!.github",
      "!node_modules/ffmpeg-static/index.js",
      "!node_modules/ffmpeg-static/package.json",
      "node_modules/ffprobe-static/index.js",
      "node_modules/ffprobe-static/package.json"
    ],
    "mac": {
      "category": "public.app-category.education",
      "extraResources": [
        {
          "from": "node_modules/ffprobe-static/ffprobe/bin/darwin/x64/"
        }
      ]
    },
    "publish": [
      {
        "provider": "github",
        "releaseType": "release"
      }
    ],
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "extraResources": [
        {
          "from": "node_modules/ffprobe-static/bin/linux/x64/ffprobe"
        }
      ]
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico",
      "extraResources": [
        {
          "from": "node_modules/ffprobe-static/bin/win32/x64/ffprobe.exe"
        }
      ]
    },
    "nsis": {}
  },
  "keywords": [],
  "author": "Miguel Ping <miguel.ping@gmail.com>",
  "devDependencies": {
    "electron": "^9.4.0",
    "electron-builder": "^22.6.0",
    "shadow-cljs": "^2.11.13",
    "source-map-support": "^0.4.18"
  },
  "dependencies": {
    "antd": "4.0.0-alpha.14",
    "create-react-class": "^15.6.2",
    "electron-dl": "^3.0.0",
    "fast-glob": "^3.2.2",
    "ffmpeg-static": "^4.2.7",
    "ffprobe-static": "^3.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "highlight.js": "10.4.1",
    "jszip": "^3.5.0",
    "md5-file": "^5.0.0",
    "normalize-path": "^3.0.0",
    "rc-field-form": "0.0.0-alpha.16",
    "rc-util": "^5.6.6",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-flip-move": "^3.0.4",
    "react-highlight.js": "^1.0.7",
    "regenerator-runtime": "^0.13.5",
    "video-react": "^0.14.1",
    "ws": "^3.2.0"
  }
}
